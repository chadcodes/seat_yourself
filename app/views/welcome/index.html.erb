<h1>Welcome!</h1>

<h2><%= link_to "See All Reservations", reservations_path %></h2>



<div class="search">

	<%= form_tag restaurants_path, :method => 'get' do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", :name => nil %>
  </p>
<% end %>


<%= form_tag(restaurants_path, method: "get") do %>
  <%= label_tag(:q, "Search for:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search", :name => nil) %>
<% end %>


</div>



